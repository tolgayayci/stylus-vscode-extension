{
    "scopeName": "source.stylus",
    "patterns": [
      {
        "name": "entity.name.struct.stylus-sdk.abi",
        "match": "\\b(Bytes|BytesSolType|ConstString|FixedBytesSolType)\\b"
      },
      {
        "name": "entity.name.trait.stylus-sdk.abi",
        "match": "\\b(AbiType|Router)\\b"
      },
      {
        "name": "keyword.other.stylus-sdk.abi",
        "match": "\\b(GenerateAbi|export)\\b"
      },
      {
        "name": "entity.name.function.stylus-sdk.block",
        "match": "\\b(basefee|chainid|coinbase|gas_limit|number|timestamp)\\b"
      }
    ],
    "grammars": [{
        "language": "rust",
        "scopeName": "source.stylus",
        "path": "src/syntaxes/stylus.tmLanguage.json"
      }],
    "fileTypes": ["rs"],
    "name": "Stylus SDK",
    "include": "source.rust"
  }
  